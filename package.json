  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.0"
  },
  "files": [
    "dist",
    "run.sh"
  ]
}
